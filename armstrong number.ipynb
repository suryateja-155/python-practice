   "source": [
    "n=int(input(\"Enter a number: \"))\n",
    "f=str(n)\n",
    "g=len(f)\n",
    "print(g)\n",
    "result=0\n",
    "for i in f:\n",
    "    result+=int(i)**g\n",
    "if result==n:\n",
    "   print(i,\"armstrong number\")\n",
    "else:\n",
    " print(i,\"Not a armstrong number\")"
   ]
